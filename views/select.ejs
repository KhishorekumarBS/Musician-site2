<html>
<head>
	<meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link href='https://fonts.googleapis.com/css?family=Abel' rel='stylesheet'>
  <link href='https://fonts.googleapis.com/css?family=Noticia Text' rel='stylesheet'>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <style type="text/css">
  	body
	{	background-image: url("mainbg.jpg");
    	background-attachment: fixed;
    	background-repeat: no-repeat;
    	background-origin: content-box;
    	background-size: 100% 100%;
	}
	.news-thumb {
  background-color: rgba(255,255,255,0.8);
  padding: 15px;
}
  </style>
</head>
<body>
	<!--Navbar-->
	   <nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>                       
      </button>
      <a class="navbar-brand" href="#">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp</a>
    </div>
    <div class="collapse navbar-collapse" id="myNavbar">
      <ul class="nav navbar-nav">
        <li><a href="/eventsinput">Add Event</a></li>
        <li class="active"><a href="/selectall">List Events</a></li>
      
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li><a href="#"><i class="fa fa-facebook-square" style="font-size:20px"></i></a></li>
        <li><a href="#"><i class="fa fa-twitter" style="font-size:20px"></i></a></li>
      </ul>
    </div>
  </div>
</nav>
<!--End Navbar-->
	<div class="container news-thumb">
		<table class="table table-bordered table-dark table-hover">
		  <thead>
    		<tr>
      			<th scope="col">#</th>
      			<th scope="col">Event Title</th>
      			<th scope="col">Date and Time</th>
      			<th scope="col">Venue</th>
   		    </tr>
  		  </thead>
  		  <tbody>
<% var count=1 %>
<% docs.forEach(function(doc){  %>
   <tr>
    <td scope="col"><%= count %></td>
     <td><%= doc._id %></td>
     <td><%= doc.dovenue.getDate()+'/' + (doc.dovenue.getMonth()+1) + '/'+doc.dovenue.getFullYear() %></td>
     <td><%= doc.Venue %></td>
   </tr>
<% count++ %>
<% }) %>
  		   </tbody>
		</table>
		<div class="display"></div>
	</div>
</body>
</html>